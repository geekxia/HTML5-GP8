/* pages/home/home.wxss */
.row {
  line-height: 500rpx;
  text-align: center;
  border-bottom: 1rpx solid #eeeeee;
}
.row text {
  display: inline-block;
  width: 50%;
}

.setting {
  padding-top: 120rpx;
}

.scroll {
  white-space: nowrap;
}
.scroll view {
  display: inline-block;
  padding: 10rpx 30rpx;
}
.scroll view .on {
  color: red;
}

.map {
  width: 100%;
  height: 400rpx;
}